{
  "name": "simplecreate",
  "displayName": "SimpleCreate Language",
  "description": "A VS Code extension for this SimpleCreate programming language.",
  "version": "0.0.2",
  "publisher": "ArhamKazimMohammed",
  "engines": {
      "vscode": "^1.60.0"
  },
  "repository": {
      "type": "git",
      "url": "https://github.com/34amohammed/simplecreate-programming-language"
  },
  "categories": [
      "Programming Languages"
  ],
  "contributes": {
    "languages": [
        {
            "id": "simplecreate",
            "aliases": ["SimpleCreate", "simplecreate"],
            "extensions": [".sc", ".simplecreate"],
            "configuration": "./language-configuration.json"
        }
    ],
    "grammars": [
        {
            "language": "simplecreate",
            "scopeName": "source.simplecreate",
            "path": "./syntaxes/simplecreate.tmLanguage.json"
        }
    ],
    "snippets": [
        {
            "language": "simplecreate",
            "path": "./snippets/simplecreate.code-snippets.json"
        }
    ]
}
}
